{
    "name": "furniture-ecommerce-frontend",
    "private": true,
    "type": "module",
    "scripts": {
      "dev": "vite",
      "build": "vite build",
      "preview": "vite preview",
      "start": "vite preview --port $PORT",
      "lint": "eslint src/**/*.{js,ts,jsx,tsx}",
      "lint:fix": "eslint src/**/*.{js,ts,jsx,tsx} --fix",
      "format": "prettier --write src/**/*.{js,ts,jsx,tsx,css}",
      "format:check": "prettier --check src/**/*.{js,ts,jsx,tsx,css}",
      "type-check": "tsc --noEmit",
      "test": "php artisan test",
      "test:watch": "php artisan test --watch"
    },
    "dependencies": {
      "axios": "^1.8.2",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "react-helmet": "^6.1.0",
      "react-helmet-async": "^2.0.5-next",
      "react-hook-form": "^7.61.0",
      "react-icons": "^5.5.0",
      "react-router-dom": "^7.7.0"
    },
    "devDependencies": {
      "@tailwindcss/forms": "^0.5.2",
      "@tailwindcss/typography": "^0.5.16",
      "@tailwindcss/vite": "^4.0.0",
      "@typescript-eslint/eslint-plugin": "^8.37.0",
      "@typescript-eslint/parser": "^8.37.0",
      "@vitejs/plugin-react": "^4.7.0",
      "alpinejs": "^3.4.2",
      "autoprefixer": "^10.4.2",
      "concurrently": "^9.0.1",
      "eslint": "^9.31.0",
      "eslint-config-prettier": "^10.1.8",
      "eslint-plugin-prettier": "^5.5.3",
      "globals": "^16.3.0",
      "laravel-vite-plugin": "^1.2.0",
      "postcss": "^8.4.31",
      "prettier": "^3.6.2",
      "tailwindcss": "^3.1.0",
      "vite": "^6.3.5"
    }
  }