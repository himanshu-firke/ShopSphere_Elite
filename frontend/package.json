{
    "$schema": "https://json.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "lint": "eslint resources/**/*.js resources/**/*.ts",
        "lint:fix": "eslint resources/**/*.js resources/**/*.ts --fix",
        "format": "prettier --write resources/**/*.js resources/**/*.ts resources/**/*.css",
        "format:check": "prettier --check resources/**/*.js resources/**/*.ts resources/**/*.css",
        "type-check": "tsc --noEmit",
