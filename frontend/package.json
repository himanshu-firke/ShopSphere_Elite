        "@vitejs/plugin-react": "^4.7.0",
        "alpinejs": "^3.4.2",
        "autoprefixer": "^10.4.2",
        "axios": "^1.8.2",
        "concurrently": "^9.0.1",
        "eslint": "^9.31.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.3",
        "globals": "^16.3.0",
        "laravel-vite-plugin": "^1.2.0",
        "postcss": "^8.4.31",
        "prettier": "^3.6.2",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "tailwindcss": "^3.1.0",
        "vite": "^6.3.5"
